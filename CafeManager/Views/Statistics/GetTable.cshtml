@model CafeManager.Application.Paging.PagedList<CafeManager.Core.StatisticsModels.TableStatisticsModel>

@{
    
}

<!DOCTYPE html>

<h1>Tables</h1>
<table class="table table-bordered table-responsive table-hover border border-0 border-dark">
    <tr>
        <th>Table Number</th>
        <th>Orders Number</th>
    </tr>
    @foreach (var t in Model)
    {
        <tr>
            <td>@t.TableId </td>
            <td>@t.OrdersCount </td>
        </tr>
    }

</table>